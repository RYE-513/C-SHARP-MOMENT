using System;

public class Rye {
    public static void Main(string[] args) {
            
        Console.WriteLine(getNum(4,5));
    
    }
        
        static int getNum(int baseNum, int powNum) {
            
            int result = 1;
            
            for(int i = 0; i < powNum; i++) {
                result = result * baseNum;
            }
            
            return result;
        }
        
    }
