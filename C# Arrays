using System;

public class Rye {
    public static void Main(string[] args) {
        
        
        
        int[] luckyNumbers = {45, 56, 8, 13};
        luckyNumbers[3] = 23; // the 3rd index changed value
        
        Console.WriteLine(luckyNumbers[3]);
        
        
        string bands = new string[3]; // 0 index number of array value to hold.
        bands[0] = "Turnover";
        bands[1] = "Title Fight";
        bands[2] = "American Football";
        
        Console.Write(bands);
        
        
        
        
    }
}
